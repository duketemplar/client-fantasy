{
  "name": "webapp-customer-registration",
  "version": "0.0.1",
  "description": "Nordnet React app",
  "config": {
    "dist": "./dist",
    "dev_server": {
      "host": "localhost",
      "port": 9000
    },
    "api_server": {
      "host": "localhost",
      "port": 9002
    }
  },
  "scripts": {
    "env:prod": "npm config set webapp-customer-registration:node_env=production",
    "env:dev": "npm config set webapp-customer-registration:node_env=development",
    "clean": "npm-run-all clean:trash clean:mkdir",
    "clean:trash": "rimraf $npm_package_config_dist",
    "clean:mkdir": "mkdirp $npm_package_config_dist",
    "prebuild": "npm run env:prod",
    "build": "npm-run-all clean webpack",
    "eslint": "eslint src",
    "eslint:jenkins": "eslint src -f checkstyle -o reports/lintresult.xml",
    "lint": "npm-run-all eslint",
    "lint:jenkins": "npm-run-all eslint:jenkins",
    "prestart": "npm run env:dev",
    "start": "NODE_ENV=development npm-run-all --parallel start:api start:webpack-dev-server",
    "start:api": "nodemon --harmony --use-strict ./api-server/api-server.js --port $npm_package_config_api_server_port",
    "start:webpack-dev-server": "node ./webpack/webpack-dev-server",
    "validate": "npm-run-all lint test",
    "webpack": "webpack --colors --progress --config ./webpack/config",
    "pretest": "npm config set webapp-customer-registration:karma_env=coverage",
    "test": "karma start",
    "pretest:debug": "npm config set webapp-customer-registration:karma_env=sourcemap",
    "test:debug": "karma start --no-single-run --reporters mocha --browsers Chrome",
    "pretest:tdd": "npm config set webapp-customer-registration:karma_env=coverage",
    "test:tdd": "karma start --auto-watch --no-single-run --reporters mocha",
    "pree2e-test": "mkdirp e2e-tests/logs && mkdirp e2e-tests/page-objects",
    "e2e-test": "BABEL_ENV=e2e node nightwatch.js",
    "pree2e-test:windows": "SET BABEL_ENV=e2e",
    "e2e-test:windows": "BABEL_ENV=e2e node nightwatch.js",
    "poste2e-test:windows": "SET BABEL_ENV=development",
    "e2e-test:server": "java -jar -Dwebdriver.chrome.driver=./e2e-tests/server/chromedriver e2e-tests/server/selenium-server-standalone-2.53.0.jar",
    "e2e-test:ci": "BABEL_ENV=e2e node nightwatch.js -e ci",
    "e2e-test:ff": "BABEL_ENV=e2e node nightwatch.js -e firefox",
    "e2e-test:ch": "BABEL_ENV=e2e node nightwatch.js -e chrome",
    "pree2e-test:install": "mkdir -p e2e-tests/server",
    "e2e-test:install": "! [ -a 'e2e-tests/server/selenium-server-standalone-2.53.0.jar' ] && npm run e2e-test:download || echo 'selenium server already installed'",
    "e2e-test:download": "cd e2e-tests/server && curl -sS -O 'http://selenium-release.storage.googleapis.com/2.53/selenium-server-standalone-2.53.0.jar'",
    "prepush": "npm run validate",
    "postbuild": "npm run copy-html",
    "copy-html": "/bin/cp ./src/index.html ./dist/init/index.html"
  },
  "dependencies": {
    "classnames": "2.2.0",
    "es6-promise": "3.0.2",
    "intl": "1.0.1",
    "nordnet-next-api": "3.0.0",
    "nordnet-ui-kit": "0.0.16",
    "normalize.css": "3.0.3",
    "react": "0.14.7",
    "react-bem-grid": "0.0.10",
    "react-dom": "0.14.7",
    "react-intl": "1.2.0",
    "react-phone-input": "1.0.2",
    "react-redux": "4.1.1",
    "react-router-redux": "4.0.2",
    "redux": "3.1.3",
    "redux-form": "4.2.0",
    "redux-thunk": "1.0.3"
  },
  "devDependencies": {
    "autoprefixer-core": "5.1.11",
    "babel-cli": "6.4.0",
    "babel-core": "6.4.0",
    "babel-eslint": "4.1.7",
    "babel-loader": "6.2.1",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-2": "6.5.0",
    "chai": "3.3.0",
    "chai-array": "0.0.2",
    "chai-as-promised": "5.2.0",
    "chai-react": "3.0.0",
    "cookies-js": "1.2.2",
    "css-loader": "0.20.2",
    "dirty-chai": "1.2.2",
    "enzyme": "2.2.0",
    "es5-shim": "4.2.0",
    "es6-promise-polyfill": "1.1.1",
    "es6-shim": "0.35.0",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "3.1.0",
    "eslint-config-nordnet": "1.1.1",
    "eslint-plugin-react": "3.15.0",
    "file-loader": "0.8.1",
    "html-loader": "0.3.0",
    "husky": "0.9.1",
    "isparta": "3.0.3",
    "isparta-loader": "0.2.0",
    "json-loader": "0.5.1",
    "karma": "0.13.9",
    "karma-chai": "0.1.0",
    "karma-chai-as-promised": "0.1.2",
    "karma-chai-sinon": "0.1.5",
    "karma-chrome-launcher": "0.2.0",
    "karma-common-js": "0.2.2",
    "karma-coverage": "0.5.0",
    "karma-es5-shim": "0.0.4",
    "karma-es6-shim": "0.2.3",
    "karma-junit-reporter": "0.3.3",
    "karma-mocha": "0.2.0",
    "karma-mocha-reporter": "1.1.1",
    "karma-phantomjs-launcher": "0.2.1",
    "karma-sourcemap-loader": "0.3.5",
    "karma-webpack": "1.7.0",
    "koa": "0.21.0",
    "koa-bodyparser": "^2.0.1",
    "koa-mount": "1.3.0",
    "koa-router": "5.1.2",
    "koa-send": "1.3.1",
    "koa-static": "1.4.9",
    "lodash": "3.10.0",
    "mkdirp": "0.5.1",
    "mocha": "2.2.4",
    "mocha-junit-reporter": "1.9.0",
    "nightwatch": "0.8.3",
    "node-libs-browser": "0.4.3",
    "node-sass": "3.3.3",
    "node-uuid": "1.4.3",
    "nodemon": "1.4.1",
    "nordnet-release-plugin": "0.0.4",
    "npm-run-all": "1.2.6",
    "opn": "1.0.2",
    "phantomjs-prebuilt": "2.1.7",
    "postcss-loader": "0.4.3",
    "react-addons-test-utils": "0.14.2",
    "react-hot-loader": "1.2.7",
    "react-router": "2.0.0",
    "redux-devtools": "2.1.5",
    "require-dir": "0.3.0",
    "rimraf": "2.4.2",
    "sass-loader": "3.0.0",
    "sinon": "1.16.1",
    "sinon-as-promised": "4.0.0",
    "sinon-chai": "2.8.0",
    "style-loader": "0.12.2",
    "template-html-loader": "0.0.3",
    "webpack": "1.12.11",
    "webpack-dev-server": "1.14.1"
  }
}
