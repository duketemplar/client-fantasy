{
  "name": "nordnet-react-app-light",
  "version": "0.0.1",
  "description": "Nordnet React app",
  "config": {
    "dist": "./dist",
    "dev_server": {
      "host": "localhost",
      "port": 9000
    },
    "api_server": {
      "host": "localhost",
      "port": 9002
    }
  },
  "scripts": {
    "clean": "npm-run-all clean:trash clean:mkdir",
    "clean:trash": "rimraf $npm_package_config_dist",
    "clean:mkdir": "mkdirp $npm_package_config_dist",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production npm run webpack",
    "eslint": "eslint src",
    "eslint:jenkins": "eslint src -f checkstyle -o reports/lintresult.xml",
    "jscs": "jscs -c .jscsrc src",
    "lint": "npm-run-all jscs eslint",
    "lint:jenkins": "npm-run-all jscs eslint:jenkins",
    "start:api": "nodemon --harmony ./api-server/api-server.js --port $npm_package_config_api_server_port",
    "start": "npm-run-all --parallel start:api start:webpack-dev-server",
    "start:webpack-dev-server": "NODE_ENV=development node ./webpack/webpack-dev-server",
    "start:windows": "set NODE_ENV=development&&node ./webpack/webpack-dev-server",
    "validate": "npm-run-all lint test",
    "webpack": "webpack --colors --progress --config ./webpack/config",
    "test": "karma start",
    "test:debug": "KARMA_ENV=sourcemap karma start --no-single-run --reporters mocha --browsers Chrome",
    "test:windebug": "set KARMA_ENV=sourcemap&&karma start --no-single-run --reporters mocha --browsers Chrome",
    "test:tdd": "karma start --auto-watch --no-single-run --reporters mocha karma.conf",
    "e2e-test": "nightwatch -r junit",
    "prepush": "npm run validate"
  },
  "dependencies": {
    "classnames": "2.2.0",
    "es6-promise": "3.0.2",
    "intl": "1.0.1",
    "nordnet-next-api": "2.0.0",
    "normalize.css": "3.0.3",
    "react": "0.13.3",
    "react-intl": "1.2.0",
    "react-redux": "^2.1.2",
    "redux": "3.0.0",
    "redux-thunk": "1.0.0"
  },
  "devDependencies": {
    "autoprefixer-core": "5.1.11",
    "babel": "5.8.23",
    "babel-core": "5.8.25",
    "babel-eslint": "4.1.3",
    "babel-jscs": "2.0.5",
    "babel-loader": "5.3.2",
    "babel-runtime": "5.8.25",
    "chai": "3.3.0",
    "chai-array": "0.0.2",
    "chai-react": "3.0.0",
    "cookies-js": "1.2.2",
    "css-loader": "0.20.2",
    "es5-shim": "4.1.15",
    "es6-promise-polyfill": "1.1.1",
    "eslint": "1.7.2",
    "eslint-config-airbnb": "0.0.7",
    "eslint-config-nordnet": "1.0.1",
    "eslint-plugin-react": "3.3.2",
    "file-loader": "0.8.1",
    "html-loader": "0.3.0",
    "husky": "0.9.1",
    "isparta": "3.0.3",
    "isparta-loader": "0.2.0",
    "jscs": "2.3.5",
    "jscs-config-nordnet": "1.0.3",
    "json-loader": "0.5.1",
    "karma": "0.13.9",
    "karma-chai-sinon": "0.1.5",
    "karma-chrome-launcher": "0.2.0",
    "karma-common-js": "0.2.2",
    "karma-coverage": "0.5.0",
    "karma-junit-reporter": "0.3.3",
    "karma-mocha": "0.2.0",
    "karma-mocha-reporter": "1.1.1",
    "karma-phantomjs-launcher": "0.2.1",
    "karma-sourcemap-loader": "0.3.5",
    "karma-webpack": "1.7.0",
    "koa": "0.21.0",
    "koa-mount": "1.3.0",
    "koa-router": "5.1.2",
    "koa-send": "1.3.1",
    "koa-static": "1.4.9",
    "lodash": "3.8.0",
    "mkdirp": "0.5.1",
    "mocha": "2.2.4",
    "nightwatch": "0.8.3",
    "node-uuid": "1.4.3",
    "nodemon": "1.4.1",
    "nordnet-release-plugin": "0.0.4",
    "npm-run-all": "1.2.6",
    "opn": "1.0.2",
    "phantomjs": "1.9.18",
    "postcss-loader": "0.4.3",
    "react-hot-loader": "1.2.7",
    "redux-devtools": "2.1.0",
    "require-dir": "0.3.0",
    "rimraf": "2.4.2",
    "sass-loader": "0.4.2",
    "sinon": "1.16.1",
    "sinon-as-promised": "4.0.0",
    "sinon-chai": "2.8.0",
    "style-loader": "0.12.2",
    "template-html-loader": "0.0.3",
    "webpack": "1.9.5",
    "webpack-dev-server": "1.8.2"
  }
}
